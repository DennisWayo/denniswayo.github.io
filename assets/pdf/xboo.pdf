                    'execution' : 'execute',
                    'retry-interval': 2000,  
                    'retry': 'never', 
                    'refresh-expired': 'manual',
                    'timeout-callback' : function(errorcode) {
                        captureErrorCode(errorcode)
                        retryTurnstileOrError('timeout');
                    },               
                    'expired-callback' : function(errorcode) {
                        captureErrorCode(errorcode)
                        retryTurnstileOrError('expired');
                    },               
                    'error-callback': function(errorcode) {
                        captureErrorCode(errorcode)
                        setTimeout( function () {
                            retryTurnstileOrError('error');
                        },2000) 
                    },
                    'callback': function (token) {
                        formSubmit();
                    }
                });
            }
        
