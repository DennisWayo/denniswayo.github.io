            function captureErrorCode(errorcode) {
                if (errorcode !== undefined && errorcode != null) {
                    window['turnstileClientErrors'].push("(" + errorcode + ")@" + new Date().toISOString())
                    unhideClientErrors()
                }
            }

            function retryTurnstileOrError(reason) {
                if(window['initialisationFailed']) {
                    return;
                }
                
                if (isSubmitted()) {
                    return;
                }
                
                window['turnstileRetry'] = window['turnstileRetry'] + 1
                try {                    
                    if (window['turnstileRetry'] <= window['maxRetries']) {
                        turnstile.reset(widgetid);
                        turnstile.execute(); 
                    } else {
                        unhideProblem(null);
                    }
                } catch(error) {
                    unhideProblem(error);
                }
            }
            
            function isDefined(data) {
                if (data === null || typeof data === "undefined") {
                    return false;
                }
                return true;
            }
            
            function renderTurnstile() {
                widgetid = turnstile.render('#turnstileForm', {
                    'sitekey': '0x4AAAAAAAIJB5yGm8wJK_4V',
